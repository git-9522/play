html,body {
    height: 100%;
}
body {
    overflow: hidden;
    position: relative;
}
/*公共样式*/
.auto {
    width: 1200px;
    margin: 0 auto;
}
/*公共样式*/
/*共用的头部*/
.header {
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 2000;
    background: #fff;
}
/*头部上面部分*/
.header_top {
    width: 100%;
    border-bottom: 1px solid #e3e3e3;
}
.header_top .top_center {
    height: 40px;
}
.top_center .t_all {
    position: relative;
}
/*左边的*/
.top_center .top_l {
    height: 40px;
    float: left;
    line-height: 40px;
}
.top_center .top_l .icon {
    width: 22px;
    height: 22px;
    position: absolute;
    left: 0;
    top: 9px;
    background: url(../images/newbie/tel.png) no-repeat;
}
.top_center .top_l span {
    margin-left: 30px;
}
/*左边的*/
/*右边的*/
.top_center .top_r {
    height: 40px;
    float: right;
}
.top_center .top_r .t_nav {
    overflow: hidden;
}
.top_center .top_r .t_nav li {
    float: left;
    margin-top: 9px;
}
.top_center .top_r .t_nav li a {
    padding: 0 15px;
    border-right: 1px solid #e3e3e3;
    color: #999999;
}
.top_center .top_r .t_nav li.t_on a {
    color: #000;
}
.top_center .top_r .t_nav li.t_in a {
    border: none;
}
.top_center .top_r .t_nav li.pic {
    padding: 0;
    margin-top: 5px;
}
.top_center .top_r .t_nav li a:hover {
    color: #ff6600;
}
.top_center .top_r .t_nav li.pic a {
    width: 22px;
    height: 22px;
    float: left;
    background-repeat: no-repeat;
    margin-top: 4px;
    padding-right: 0px;
}
.top_center .top_r .t_nav li.pic a.phone {
    width: 83px;
    height: 27px;
    margin-top: 0px;
    background-image: url(../images/newbie/phone1.png); 
} 
.top_center .top_r .t_nav li.pic a.weixin {
    background-image: url(../images/newbie/weixin.png);
} 
.top_center .top_r .t_nav li.pic a.weibo {
    background-image: url(../images/newbie/weibo.png);
} 
/*右边的*/
/*头部上面部分*/
/*头部下部分*/
.header_bottom {
    height: 90px;
}
.header_bottom .logo {
    float: left;
}
.header_bottom .logo a {
    display: block;
    width: 367px;
    height: 56px;
    background: url(../images/newbie/logo1.png) no-repeat;
    margin-top: 20px;
}
.header_bottom .nav {
    float: right;
    margin-top: 28px;
    overflow: hidden;
}
.header_bottom .nav li {
    float: left;
    margin-right: 20px;
}
.header_bottom .nav li a {
    display: block;
    padding: 5px 10px;
    border-radius: 5px;
    color: #000;
    font-size: 16px;
}
.header_bottom .nav li.active a {
    color: #fff;
    background: #ff6600;
}
/*头部下部分*/
/*container部分*/
/*swiper部分*/
.swiper_wrapper {
    width: 100%;
    height: 100%;
}
.swiper_wrapper>li {
    height: 100%;
    width: 100%;
}
/*swiper1部分*/
.swiper_wrapper .swiper1 {
    background: #ffe3ac;
    height: 100%;
    position: relative;
    overflow: hidden;
}
.s_center {
    height: 100%;
    position: relative;
}
.s_center .s1_com {
    width: 502px;
    height: 309px;
    position: absolute;
    left: 260px;
    bottom: 400px;
    opacity: 0;
    z-index: 100;
}
.s_center .s1_icon li {
    width: 40px;
    height: 45px;
    background-repeat: no-repeat;
    background-position:  center;
    position: absolute;
    left: 0;
    bottom: 385px;
    opacity: 0;
}
.s_center .s1_zh li {
    width: 99px;
    height: 140px;
    background-repeat: no-repeat;
    background-position:  center;
    position: absolute;
    bottom: 425px;
    right: 0;
    opacity: 0;
}
.wave li {
    height: 78px;
    width: 100%;
    background-repeat: no-repeat;
    background-image: url(../images/newbie/lang1.png);
    position: absolute;
    bottom: 0;
    left: 0;
}
.wave li:nth-child(1){
    animation: omove1 2s  linear infinite;
}
.wave li:nth-child(2){
    animation: omove2 2s  linear infinite;
}
.wave li:nth-child(3){
    animation: omove3 2s  linear infinite;
}
@keyframes omove1 {
    25% {
        transform: translateY(3px);
    }
    50% {
        transform: translateY(6px);
    }
    75% {
        transform: translateY(3px);
    }
    100% {
        transform: translateY(0px);
    }
}
@keyframes omove2 {
    25% {
        transform: translateY(-3px);
    }
    50% {
        transform: translateY(3px);
    }
    75% {
        transform: translateY(-3px);
    }
    100% {
        transform: translateY(0px);
    }
}
@keyframes omove3 {
    25% {
        transform: translateY(-3px);
    }
    50% {
        transform: translateY(-6px);
    }
    75% {
        transform: translateY(-3px);
    }
    100% {
        transform: translateY(0px);
    }
}
.s_center .cloud1 {
    width: 145px;
    height: 57px;
    position: absolute;
    bottom: 550px;
    left: 90px;
    background: url(../images/newbie/cloud1.png) no-repeat;
    animation: ocloud1 15s  linear infinite;
    opacity: 0;
}
.s_center .cloud2 {
    width: 145px;
    height: 57px;
    position: absolute;
    bottom: 220px;
    right: 100px;
    background: url(../images/newbie/cloud2.png) no-repeat;
    animation: ocloud2 20s  linear infinite;
    opacity: 0;
    z-index: 50;
}
@keyframes ocloud1 {
    50% {
        transform: translateX(200px);
        opacity: 1;
    }
    100% {
        transform: translateX(400px);
        opacity: 0;
    }
}
@keyframes ocloud2 {
    50% {
        transform: translateX(-200px);
        opacity: 1;
    }
    100% {
        transform: translateX(-400px);
        opacity: 0;
    }
}
