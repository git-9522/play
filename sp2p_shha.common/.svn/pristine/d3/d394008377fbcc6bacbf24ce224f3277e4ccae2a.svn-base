.ne_fina_nav {
    width: 100%;
    height: 50px;
    background: #fe6467;
}
.ne_fina_ncen {
    width: 1200px;
    height: 50px;
    margin: 0 auto;
}
.ne_ncen_list>li {
    height: 50px;
    line-height: 50px;
    font-size: 16px;
}
.ne_ncen_list>li.ncen_list_act {
    background: #fa8082;
}
.ne_ncen_list>li a {
    display: block;
    width: 100%;
    height: 100%;
    color: #fff;
    padding: 0 15px;
}
.ne_ncen_list>li:hover a {
    color: #fff;
}
.ne_fina_main {
    width: 1200px;
    margin: 0 auto;
}
.ne_fina_cen {
    width: 1198px;
    margin: 0 auto;
    border: 1px solid #e7e7e7;
    padding-top: 20px;
    background: #fff;
    padding-bottom: 65px;
}
.ne_fina_list {
    width: 1130px;
    margin: 0 auto;
}
.ne_fina_list>li {
    float: left;
    border: 1px solid #e7e7e7;
    height: 30px;
    line-height: 30px;
    border-radius: 4px;
    margin-right: 30px;
    width: 80px;
    color: #666;
    text-align: center;
    cursor: pointer;
}
.ne_fina_list>li.fina_list_act {
    border: 1px solid #ff5256;
    color: #ff5256;
}
.ne_fina_font {
    padding: 0 35px;
    margin-top: 10px;
}
.ne_fina_font>a {
    display: block;
    height: 50px;
    line-height: 50px;
    border-bottom: 1px dashed #e7e7e7;
}
.ne_fina_font>a>p {
    width: 500px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
