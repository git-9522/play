/*my_gai_nav*/
.gai_nav>li{
    height: 70px;
    line-height: 70px;
    margin-left: 40px;
    font-size: 18px;
    padding-top: 20px;
    float: left;
}
.gai_nav>li>a {
    display: block;
    line-height: 35px;
}
.gai_nav>li.ord {
    height: 70px;
    border-bottom: 5px solid #25a4e7; 
}
.gai_nav>li.ord a{
    color: #25a4e7;
}
.gai_nav>li a {
    color: #677887;
}
.gai_nav>li.gai_nav_act {
    margin-left: 20px;
}
.gai_nav>li.gai_nav_act>a {
    width: 120px;
    line-height: 35px;
    text-align: center;
    height: 35px;
    border: 1px solid transparent;
    border-radius: 5px;
    background: #fff url(../images/jian_down.png) no-repeat 105px center;
}
.gai_nav>li.gai_nav_act:hover>a {
    border-radius: 5px 5px 0 0;
    background: #25a4e7 url(../images/jian_up.png) no-repeat 105px center;
    color: #fff;
}
.gai_nav>li.gai_nav_act:hover .gai_dropdown {
    display: block;
}
.gai_dropdown {
    left: 0;
    top: 56px;
    border: 1px solid #25a4e7;
    padding-top: 10px;
    width: 120px;
    text-align: center;
    border-radius: 0 0 5px 5px;
    display: none;
    z-index: 19999;
    background: #fff;
}
.gai_dropdown>ul>li {
    line-height: 35px;
}
.gai_dropdown>ul>li>a {
    display: block;
    color: #25a4e7;
}
.gai_nav>li.gai_nav_act .gai_dropdown>ul>li>a:hover{
    background: #25a4e7;
    color: #fff;
}
