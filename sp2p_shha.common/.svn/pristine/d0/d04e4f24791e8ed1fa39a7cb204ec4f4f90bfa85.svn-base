/* Download  */
body,ul,ol,li,p,h1,h2,h3,h4,h5,h6,table,td,th,form,fieldset,img,dl,dt,dd{margin:0px;padding:0px; font-family:'Microsoft yahei';}
h1 {
    color: #000;
    font-size: 30px;
    font-weight: normal;
    text-align: center;
}
a{ color:#35679a; text-decoration:none;}
a:hover{ color:#c00; text-decoration:underline;}
img{ border:none;}
li{ list-style:none;}
.survey {
            width: 100%;
            background: #cce6f5;
            padding-bottom: 100px;
        }
        .survey_bg {
            width: 1200px;
            height: 424px;
            background: url(../images/questionnaire-survey_open.jpg) no-repeat center;
            margin: 0 auto;
        }
        .survey_main {
            width: 1200px;
            margin: 0 auto;
            background: #fff;
            padding-bottom: 80px;
        }
.issue{ width:1000px; height:auto; z-index:99; margin: 0 auto 20px;}
.issue h3{ line-height:24px; font-size:20px; color:#fff; font-weight:normal; background: #00b7ee; padding: 10px 20px; margin-bottom: 10px;}
.issue ul{ padding:4px 10px;}
.issue li{ margin-bottom:3px; line-height:24px; min-height:24px; padding:1px 0 4px; border:1px solid #fff;}
.issue li input{ display:none;}
.issue li span.rad{ float:left; clear:left; width:22px; height:22px; overflow:hidden; margin:3px 3px 0 0; background:url(../images/questionnaire-bg.png) 0 0 no-repeat;}
.line_tip {
    width: 400px;
    height: 30px;
    float: right;
    margin-left: 50px;
    position: relative;
}
.line_tip .line_box {
    width: 300px;
    height: 10px;
    border: 1px solid #00b7ee;
    position: absolute;
    left: 0;
    top: 9px;
}
.line_tip .line_box .line {
    position: absolute;
    left: 0;
    top: 0;
    height: 10px;
    width: 100px;
    background: #00b7ee;
}
.line_tip .line_num {
    position: absolute;
    right: 0;
    top: 3px;
    color: #00b7ee;
    font-size: 14px;
}
.issue li:hover span.rad,
.issue li.hover span.rad{ background:url(../images/questionnaire-bg.png) 0 -22px no-repeat;}
.issue li label{ float:left; width:370px; color:#555; font-size:14px; margin-top:3px;}
.issue div.selected li.sel{ border:1px solid #ccc;}
.issue li.sel span.rad,
.issue div.selected li.sel:hover span{ background:url(../images/questionnaire-bg.png) 0 -44px no-repeat;}
.check_btn {
    width: 140px;
    height: 40px;
    border-radius: 3px;
    background: #00b7ee;
    color: #fff;
    line-height: 40px;
    text-align: center;
    margin: 80px auto 0;
    cursor: pointer;
}
.hide {
    display: none;
}
.pass {
   width: 1000px;
    line-height: 30px;
    margin: 30px auto 0;
    font-size: 18px;
    padding-left: 10px; 
    color: #ff6600;
}
.tishi {
    width: 1000px;
    line-height: 30px;
    margin: 30px auto 0;
    font-size: 18px;
    padding-left: 10px;
}
.tishi>span {
    color: #ff6600;
}
