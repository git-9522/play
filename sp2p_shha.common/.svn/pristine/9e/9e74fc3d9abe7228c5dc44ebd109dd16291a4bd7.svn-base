/*按钮动画*/
.waveRaidus {
    -webkit-animation: waveRaidus 2s ease-out 0s infinite;
    animation: waveRaidus 2s ease-out 0s infinite;
    opacity: 0;
}
@-webkit-keyframes waveRaidus {
     0% {
            transform: scale(1);
            opacity: 0;
        }
        65% {
            transform: scale(1);
            opacity: 0.6;
        }
        66% {
            transform: scale(1);
            opacity: 0.8;
        }
        100% {
            transform: scale(1.3);
            opacity: 0;
        }
}
@keyframes waveRaidus {
     0% {
            transform: scale(1);
            opacity: 0;
        }
        65% {
            transform: scale(1);
            opacity: 0.6;
        }
        66% {
            transform: scale(1);
            opacity: 0.8;
        }
        100% {
            transform: scale(1.3);
            opacity: 0;
        }
}
