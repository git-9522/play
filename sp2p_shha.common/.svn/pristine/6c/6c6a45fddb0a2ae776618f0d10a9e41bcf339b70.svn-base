    left: 0;
    background: url(../images/new_bank_l.png) no-repeat center;
}
.bd5>i.bd5_left:hover {
    background: url(../images/new_bank_l1.png) no-repeat center;
}
.bd5>i.bd5_right {
    right: 0;
    background: url(../images/new_bank_r.png) no-repeat center;
