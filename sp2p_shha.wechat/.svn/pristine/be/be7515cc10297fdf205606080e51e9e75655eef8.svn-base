@charset "utf-8";
/* webrx CSS Document */
/*reset*/
html{font-size:100%;-webkit-text-size-adjust:100%;-ms-text-size-adjust:100%}
html, body, div, span, p, h1, h2, h3, h4, h5, h6, em, img, strong, blockquote, sub, sup, tt, i, b, dd, dl, dt, form, label, table, caption, tbody, tfoot, thead, tr, th, td, ul, li, p, a, ol, ul, input { margin: 0; padding: 0 }
table { border-collapse: collapse; border-spacing: 0 }
img { border: 0; vertical-align:middle;width: 100%; max-width: 100%; }
em, i { font-style: normal }
ul, ol, li { list-style-type: none }
h1, h2, h3, h4, h5, h6 { font-size: 100%; font-weight: 400 }
button, input, select, textarea { font-size: 100%; vertical-align: middle }
textarea{resize:none}
a, a:visited { color: #333; text-decoration: none }
a, iframe, input, select { outline: 0 none }
::-webkit-scrollbar{width: 0;}
.clear { clear: both; font-size: 0; line-height: 0; height: 0 }
.clearfix:after { content: "\200B"; display: block; height: 0; clear: both }
.clearfix {zoom:1; clear: both; }
.left { float: left; }
.right { float: right; }
html { height: 100%;font-size:16px;  }
input[type="text"] {-webkit-appearance: none;}
textarea{-webkit-appearance: none;}
:root ,body{height:100%;}
body { font-family:Helvetica; background-color:#fff; color:#333;display:-webkit-box;display:box;-webkit-box-orient:vertical;}
body{-webkit-tap-highlight-color:rgba(0,0,0,0)}
input[type="text"] {-webkit-appearance: none;}
textarea{-webkit-appearance: none;}
input{-webkit-appearance: none;}

/**push**/
.push_safe{ border-top:0.5rem solid #eef2f5; border-bottom:0.5rem solid #eef2f5;}
.push_safe li{ float:left; width:25%;}
.push_safe .menu_ico{ display:block; width:100%; height:auto;}
.push_new{ padding:0 0.75rem;border-bottom:0.5rem solid #eef2f5;}
.push_new .title{ font-size:1.125rem; text-align:center; padding:0.6rem 0;}
.push_new .cont{ overflow:hidden;}
.push_new .new_one{ float:left; width:48.3%; margin-right:3.4%;}
.push_new .new_two{ float:right; width:48.3%;}
.push_new .new_three{ float:left; width:100%; margin:0.625rem 0;}
.push_lce{ margin:0 0.75rem 2rem; border-bottom:1px solid #e7e7e7;}
.push_lce .title{ font-size:1.125rem; text-align:center; padding:0.6rem 0;}
.push_lce li{ width:48.3%; float:left; margin-right:3.4%; text-align:center;}
.push_lce li span{ display:block; line-height:1.2rem; margin:0.5rem 0; font-size:0.812rem;}



@media screen and (max-width:321px) {
html { font-size: 87.5%; }
}
@media screen and (min-width:321px) and (max-width:375px) {
html { font-size:100%; }
}
@media screen and (min-width:376px) and (max-width:414px) {
html { font-size: 100% }

}
@media screen and (min-width:415px) and (max-width:639px) {
html { font-size: 125%; }

}
@media screen and (min-width:750px) and (max-width:799px) {
html {font-size:140%;}
}
 @media screen and (min-width:800px) {
html { font-size: 150%; }
}


