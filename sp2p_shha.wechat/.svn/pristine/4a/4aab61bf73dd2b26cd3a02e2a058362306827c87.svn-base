#{if userInvestDetail}
<li class="flex">
	<div>投资金额</div>
	<div class="flex-1 tr">${userInvestDetail?.amount?.format(common.constants.Constants.FINANCE_FORMATE_NORMAL)}元</div>
</li>
<li class="flex">
	<div>年利率</div>
	<div class="flex-1 major tr">
		<span class="f18">${userInvestDetail?.apr}</span>%
	</div>
</li>
<li class="flex">
	<div>期限</div>
	<div class="flex-1 tr">${userInvestDetail?.period}${userInvestDetail?.period_unit?.getShowValue()}</div>
</li>
<li class="flex">
	<div>还款方式</div>
	<div class="flex-1 tr">${userInvestDetail?.repayment_type?.value}</div>
</li>
<li class="flex">
	<div>放款时间</div>
	<div class="flex-1 tr">${userInvestDetail?.release_time?.format(common.constants.Constants.DATE_FORMATE)?:'----'}</div>
</li>
<li class="flex">
	<div>状态</div>
	<div class="flex-1 tr">${userInvestDetail?.status?.value}</div>
</li>
<li class="tr">
	#{if canbeTransferd}
		<a class="contract2" href="@{wechat.front.account.MyFundCtrl.applyDebtPre(userInvestDetail?.sign)}">转让</a>
	#{/if}
	#{else}
		<a class="contract2 gray" >转让</a>
	#{/else}
	&nbsp;
	#{if models.core.entity.t_bid.Status.REPAYING.equals(userInvestDetail?.status) || models.core.entity.t_bid.Status.REPAYED.equals(userInvestDetail?.status)}
		<a href="@{wechat.front.account.MyFundCtrl.showBidPactPre(userInvestDetail?.bidIdSign)}" class="contract2">合同</a>
	#{/if}
	#{else}
		<a class="contract2 gray">合同</a>
	#{/else}
#{/if}
